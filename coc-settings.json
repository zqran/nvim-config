{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "j--avascript",
    "t--ypescript",
    "t--ypescriptreact",
    "j--son",
    "j--avascriptreact"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "eslint.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "html"
  ],
  "eslint.options": {
    "configFile": ".eslintrc.js"
  },
  "prettier.singleQuote": true,
  "prettier.semi": false
}
